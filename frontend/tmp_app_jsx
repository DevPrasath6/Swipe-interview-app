function InterviewSession() {
  _s();
  const [stage, setStage] = useState("welcome");
  const [currentQuestionIndex, setCurrentQuestionIndex] = useState(0);
  const [answers, setAnswers] = useState([]);
  const [currentAnswer, setCurrentAnswer] = useState("");
  const [timer, setTimer] = useState(0);
  const [isTimerActive, setIsTimerActive] = useState(false);
  const [candidate, setCandidate] = useState({ name: "", email: "", phone: "" });
  const [uploadedFile, setUploadedFile] = useState(null);
  const timerRef = useRef();
  const startTimer = (timeLimit) => {
    setTimer(timeLimit);
    setIsTimerActive(true);
  };
  const stopTimer = () => {
    setIsTimerActive(false);
    clearTimeout(timerRef.current);
  };
  const handleSubmitAnswer = () => {
    const currentQuestion = QUESTIONS[currentQuestionIndex];
    const score = Math.floor(Math.random() * 40) + 60;
    const newAnswer = {
      questionId: currentQuestion.id,
      question: currentQuestion.text,
      answer: currentAnswer,
      score,
      timeSpent: currentQuestion.timeLimit - timer,
      difficulty: currentQuestion.difficulty
    };
    const updatedAnswers = [...answers, newAnswer];
    setAnswers(updatedAnswers);
    setCurrentAnswer("");
    stopTimer();
    if (currentQuestionIndex < QUESTIONS.length - 1) {
      setCurrentQuestionIndex(currentQuestionIndex + 1);
      startTimer(QUESTIONS[currentQuestionIndex + 1].timeLimit);
    } else {
      completeInterview(updatedAnswers);
    }
  };
  const completeInterview = (finalAnswers) => {
    const totalScore = finalAnswers.reduce((sum, answer) => sum + answer.score, 0) / finalAnswers.length;
    const candidateData = {
      ...candidate,
      id: Date.now(),
      score: Math.round(totalScore),
      answers: finalAnswers,
      completedAt: (/* @__PURE__ */ new Date()).toISOString(),
      status: "completed"
    };
    const existingCandidates = JSON.parse(localStorage.getItem("candidates") || "[]");
    existingCandidates.push(candidateData);
    localStorage.setItem("candidates", JSON.stringify(existingCandidates));
    setStage("completed");
  };
  const handleTimeUp = () => {
    stopTimer();
    handleSubmitAnswer();
  };
  useEffect(() => {
    if (isTimerActive && timer > 0) {
      timerRef.current = setTimeout(() => {
        setTimer(timer - 1);
      }, 1e3);
    } else if (isTimerActive && timer === 0) {
      handleTimeUp();
    }
    return () => clearTimeout(timerRef.current);
  }, [timer, isTimerActive]);
